{
    "version": 2,
    "builds": [
        {
            "src": "api/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/test",
            "dest": "api/server.js"
        },
        {
            "src": "/api/chat",
            "dest": "api/server.js"
        },
        {
            "src": "/api/postmark/(.*)",
            "dest": "api/server.js"
        },
        {
            "src": "/api/wave/(.*)",
            "dest": "api/server.js"
        },
        {
            "src": "/api/wix/(.*)",
            "dest": "api/server.js"
        },
        {
            "src": "/api/cloudinary/(.*)",
            "dest": "api/server.js"
        },
        {
            "src": "/api/google/(.*)",
            "dest": "api/server.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
} 